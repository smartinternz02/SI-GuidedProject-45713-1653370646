public class RandomContactFactory {
    public static List<Contact> generateRandomContacts(Integer numnct,string lastname){
        List<Contact>contacts =new List<Contact>();
        for(Integer i=0;i<numnct;i++){
            Contact cnt=new Contact(FirstName= 'Test '+i,LastName=lastname);
            contacts.add(cnt);
        }
        return contacts;
    }
}
